{
  "name": "trailerify",
  "description": "Chrome extension to know IMDB ratings of a particular movie on Trailers Addict",
  "version": "0.0.2",
  "short_name": "trailerify",
  "content_scripts": [
    {
      "matches": ["*://www.traileraddict.com/*"],
      "js": [ "dist/js/main.js"],
      "css": ["dist/css/styles.css"]
    }
  ],
  "browser_action": {
      "default_title": "trailerify",
      "default_icon": "dist/img/icon.png"
  },
  "manifest_version": 2
}
